              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "income": {
                      "type": "string",
                      "enum": [
                        "유리",
                        "적정",
                        "불리",
                        "측정불가"
                      ]
                    }
                  },
                  "required": [
                    "income"
                  ]
                },
                "examples": {
